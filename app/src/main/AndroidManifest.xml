<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.industrialmaster.dayplanner">

    <application
        android:allowBackup="true"
        android:icon="@drawable/planss"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SpecialContactsAddActivity"></activity>
        <activity
            android:name=".SpecialContactsActivity"
            android:label="@string/title_activity_special_contacts"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MyProfileActivity" />
        <activity
            android:name=".OfficeTaskAddActivity"
            android:label="@string/title_activity_office_task_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OfficeTaskActivity"
            android:label="@string/title_activity_office_task"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DoctorAppointmentAddActivity" />
        <activity
            android:name=".DoctorAppointmentActivity"
            android:label="@string/title_activity_doctor_appointment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MeetingsAddActivity" />
        <activity
            android:name=".MeetingsActivity"
            android:label="@string/title_activity_meetings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>